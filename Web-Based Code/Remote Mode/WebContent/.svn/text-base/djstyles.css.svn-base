#nameDuration{ float: left; min-height: 160px; width:400px; margin-left: 40px;}
#create{ margin-top: 180px; margin-left:350px; margin-bottom: 20px}

#uploadForm{height: 90%;width: 100%;}
#frame{height: 100%;width: 100%;}
#dialog{visibility: hidden;}